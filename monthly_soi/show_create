CREATE EXTERNAL TABLE `stp_lte_sdr_agg_monthly`(
  `trans_mth` string,
  `mdn` string,
  `usagetype` string,
  `enb` string)
PARTITIONED BY (
  `trans_mnth` string)
ROW FORMAT DELIMITED
  FIELDS TERMINATED BY '|'
  LINES TERMINATED BY '\n'
STORED AS INPUTFORMAT
  'org.apache.hadoop.mapred.TextInputFormat'
OUTPUTFORMAT
  'org.apache.hadoop.hive.ql.io.HiveIgnoreKeyTextOutputFormat'
LOCATION
  'hdfs://ey9hdpnpuat/data/npicem/stp/LTE/stp_lte_sdr_agg_monthly'
TBLPROPERTIES (
  'transient_lastDdlTime'='1569432125')
